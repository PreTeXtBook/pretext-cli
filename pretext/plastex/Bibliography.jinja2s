name: thebibliography
<references>
{% for item in obj %}
  <biblio xml:id="{{ item.id }}">{{ item.bibcite }}</biblio>
{% endfor %}
</references>

name: bibliography
<div class="bibliography">
<h1>{{ obj.title }}</h1>
{{ obj }}
</div>

name: cite
<!-- \cite -->
{% for item in obj.citation() %}
  {% if item.idref and item.idref.bibitem.url %}
  <!-- a href="{{ item.idref.bibitem.url }}" {{ item }} -->
  {% else %}
  <xref ref="{{ item }}"/>
  {% endif %}
{% endfor %}

