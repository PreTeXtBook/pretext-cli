{% set toc = doc.tableofcontents %}

<?xml version="1.0" encoding="utf-8"?>

<chapter xml:id="{{ obj.id }} xml:lang="en-US" xmlns:xi="http://www.w3.org/2001/XInclude">




<book xml:id="{{ obj.id }}" >
    <title>{{ obj.title }}</title>


    <!-- Include frontmatter -->
