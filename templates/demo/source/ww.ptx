<?xml version='1.0' encoding='utf-8'?>
<webwork>
    <pg-code>
        $a = Compute(random(1, 9, 1));
        $b = Compute(random(1, 9, 1));
        $c = $a + $b;
    </pg-code>
    <statement>
        <p> Compute the sum of <m>
                <var name="$a" />
            </m> and <m>
                <var name="$b" />
            </m>: </p>
        <p>
            <m><var name="$a" /> + <var name="$b" /> =</m>
            <var name="$c" width="5" />
        </p>
    </statement>
    <hint>
        <p> Add <m>
                <var name="$a" />
            </m> and <m>
                <var name="$b" />
            </m> together. </p>
    </hint>
    <solution>
        <p><m><var name="$a" /> + <var name="$b" /> = <var name="$c" /></m>. </p>
    </solution>
</webwork>