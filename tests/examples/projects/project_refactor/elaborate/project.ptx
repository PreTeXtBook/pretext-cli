<?xml version="1.0" encoding="utf-8"?>
<project
  ptx-version="2"
  source="my_ptx_source"
  publication="dont-touch"
  output-dir="build/here"
  stage="build/here/staging"
  site="my-great-site"
  xsl="customizations"
  asy-method="local">
  <server name="asy" url="http://example1.com" />
  <server name="sage" url="http://example2.com" />
  <targets>
    <target
      name="web"
      format="html"
      source="book.ptx"
      output-dir="web"
      deploy-dir=""
      xsl="silly.xsl"
      asy-method="server" />
    <target
      name="print"
      format="pdf"
      publication="extras/print.xml"
      output-dir="my-pdf"
      output-filename="out.pdf">
      <server name="asy" url="http://example3.com" />
      <stringparams foo="bar" baz="goo" />
    </target>
    <target
      name="no-deploy-dir"
      format="html"
      deploy="yes">
      <server name="asy" url="http://example3.com" />
      <stringparams foo="bar" baz="goo" />
    </target>
  </targets>
</project>
